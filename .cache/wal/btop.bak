# Main background, empty for terminal default, need to be empty if you want transparent background
theme[main_bg]="#171717"

# Main text color
theme[main_fg]="#3c3c3d"

# Title color for boxes
theme[title]="#3c3c3d"

# Highlight color for keyboard shortcuts
theme[hi_fg]="#523792"

# Background color of selected item in processes box
theme[selected_bg]="#524CB8"

# Foreground color of selected item in processes box
theme[selected_fg]="#3c3c3d"

# Color of inactive/disabled text
theme[inactive_fg]="#40"

# Color of text appearing on top of graphs, i.e uptime and current network graph scaling
theme[graph_text]="#3c3c3d"

# Background color of the percentage meters
theme[meter_bg]="#40"

# Misc colors for processes box including mini cpu graphs, details memory graph and details status text
theme[proc_misc]="#6C9AC0"

# Cpu box outline color
theme[cpu_box]="#6C9AC0"

# Memory/disks box outline color
theme[mem_box]="#6C9AC0"

# Net up/down box outline color
theme[net_box]="#6C9AC0"

# Processes box outline color
theme[proc_box]="#6C9AC0"

# Box divider line and small boxes line color
theme[div_line]="#6C9AC0"

# Temperature graph colors
theme[temp_start]="#524CB8"
theme[temp_mid]="#906B9E"
theme[temp_end]="#A667D3"

# CPU graph colors
theme[cpu_start]="#524CB8"
theme[cpu_mid]="#906B9E"
theme[cpu_end]="#A667D3"

# Mem/Disk free meter
theme[free_start]="#524CB8"
theme[free_mid]="#906B9E"
theme[free_end]="#A667D3"

# Mem/Disk cached meter
theme[cached_start]="#524CB8"
theme[cached_mid]="#906B9E"
theme[cached_end]="#A667D3"

# Mem/Disk available meter
theme[available_start]="#524CB8"
theme[available_mid]="#906B9E"
theme[available_end]="#A667D3"

# Mem/Disk used meter
theme[used_start]="#524CB8"
theme[used_mid]="#906B9E"
theme[used_end]="#A667D3"

# Download graph colors
theme[download_start]="#524CB8"
theme[download_mid]="#906B9E"
theme[download_end]="#A667D3"

# Upload graph colors
theme[upload_start]="#524CB8"
theme[upload_mid]="#906B9E"
theme[upload_end]="#A667D3"

# Process box color gradient for threads, mem and cpu usage
theme[process_start]="#524CB8"
theme[process_mid]="#906B9E"
theme[process_end]="#A667D3"
