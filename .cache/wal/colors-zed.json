{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#17171790",
                "editor.background": "#17171790",
                "editor.foreground": "#3c3c3c",
                "text": "#3c3c3c",
                "text.muted": "#3c3c3c70",
                "text.ignored": "#3c3c3c40",
                "text.placeholder": "#3c3c3c50",
                "ignored": "#3c3c3c30",
                "element.hover": "#3c3c3c30",
                "ghost_element.hover": "#A3493F30",
                "ghost_element.selected": "#A3493F30",
                "ghost_element.active": "#A3493F60",
                "border": "#171717",
                "editor.highlighted_line.background": "#A3493F10",
                "editor.active_line.background": "#A3493F10",
                "panel.background": "#17171790",
                "title_bar.background": "#17171790",
                "title_bar.inactive_background": "#17171790",
                "status_bar.background": "#17171790",
                "drop_target.background": "#17171790",
                "elevated_surface.background": "#171717",
                "toolbar.background": "#17171790",
                "tab_bar.background": "#17171790",
                "tab.inactive_background": "#17171790",
                "tab.active_background": "#A3493F30",
