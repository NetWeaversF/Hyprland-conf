                "terminal.ansi.bright_cyan": "#c980c1",
                "terminal.ansi.bright_white": "#3c3c3c",
                "modified": "#A3493F",
                "syntax": {
                    "attribute": {
                        "color": "#A3493F"
                    },
                    "boolean": {
                      "color": "#48A34E"
                    },
                    "comment": {
                      "color": "#b6b6b6"
                    },
                    "comment.doc": {
                      "color": "#b6b6b6"
                    },
                    "constant": {
                      "color": "#75ba7a"
                    },
                    "function": {
                      "color": "#c980c1"
                    },
                    "keyword": {
                      "color": "#232299"
                    },
                    "number": {
                      "color": "#4446BB"
                    },
                    "operator": {
                      "color": "#232299"
                    },
                    "string": {
                      "color": "#A3493F"
                    },
                    "variable": {
                      "color": "#48A34E"
                    }
                },
                "players": [
                  {
                    "cursor": "#A3493F",
                    "background": "#171717",
                    "selection": "#A3493F30"
                  }
                ]
