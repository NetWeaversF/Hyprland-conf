                "scrollbar.track.border": "#171717",
                "scrollbar.thumb.background": "#A3493F",
                "editor.gutter.background": "#17171790",
                "terminal.background": "#17171710",
                "terminal.foreground": "#3c3c3c",
                "terminal.dim_foreground": "#3c3c3c",
                "terminal.bright_foreground": "#3c3c3c",
                "terminal.ansi.black": "#171717",
                "terminal.ansi.red": "#A3493F",
                "terminal.ansi.green": "#48A34E",
                "terminal.ansi.yellow": "#C09D50",
                "terminal.ansi.blue": "#232299",
                "terminal.ansi.magenta": "#4446BB",
                "terminal.ansi.cyan": "#B756AD",
                "terminal.ansi.white": "#797979",
                "terminal.ansi.bright_black": "#b6b6b6",
                "terminal.ansi.bright_red": "#ba766f",
                "terminal.ansi.bright_green": "#75ba7a",
                "terminal.ansi.bright_yellow": "#cfb57b",
